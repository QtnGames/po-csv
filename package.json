{
  "name": "po-csv",
  "version": "1.0.0",
  "description": "Convert GNU gettext PO files from CSV files and merge them back in.",
  "bin": {
    "pocsv": "index.js"
  },
  "scripts": {
    "test": "npm run-script test-po-csv && npm run-script test-csv-po",
    "test-po-csv": "./index.js tests/input.po | diff - tests/output.csv",
    "test-csv-po": "./index.js tests/input.po tests/translated.csv | diff - tests/translated.po"
  },
  "keywords": [
    "gettext",
    "po",
    "csv",
    "translation",
    "i18n"
  ],
  "author": "Marek ‘saji’ Augustynowicz",
  "license": "MIT",
  "dependencies": {
    "fast-csv": "^0.5.5",
    "pofile": "^0.2.12",
    "promise": "^6.1.0"
  }
}
